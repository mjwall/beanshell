                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>engine/src</package-root>
                <classpath mode="compile">classes</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">lib/servlet.jar:classes</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>tests/junitTests/src</package-root>
                <unit-tests/>
                <classpath mode="compile">lib/junit-4.8.2.jar:classes</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
